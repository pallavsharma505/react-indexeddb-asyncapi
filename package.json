{
  "name": "react-indexeddb-asyncapi",
  "version": "0.1.6",
  "description": "A TypeScript utility class for Vite-React projects that simplifies IndexedDB operations with async/await syntax, comprehensive error handling, and automatic retry mechanisms.",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/pallavsharma505/react-indexeddb-asyncapi"
  },
  "homepage": "https://github.com/pallavsharma505/react-indexeddb-asyncapi/wiki",
  "bugs": {
    "url": "https://github.com/pallavsharma505/react-indexeddb-asyncapi/issues"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "indexeddb",
    "asyncapi",
    "typescript",
    "vite",
    "utility",
    "database",
    "async-await",
    "idb",
    "error-handling",
    "retry",
    "recovery",
    "monitoring",
    "circuit-breaker"
  ],
  "author": "Pallav Sharma <pallavsharma505@gmail.com>",
  "contributors": [
    {
      "name": "Pallav Sharma",
      "email": "pallavsharma505@gmail.com"
    }
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "jest"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "rollup": "^3.29.5",
    "rollup-plugin-typescript2": "^0.34.1",
    "typescript": "^5.0.0"
  }
}
